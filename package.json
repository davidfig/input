    "license": "MIT",
    "bugs": {
        "url": "https://github.com/davidfig/input/issues"
    },
    "homepage": "https://github.com/davidfig/input#readme",
    "devDependencies": {
        "fork-me-github": "^1.0.2",
        "highlight.js": "^9.12.0",
        "pixi-ease": "^0.11.0"
    },
    "dependencies": {
        "eventemitter3": "^2.0.3"
    }
