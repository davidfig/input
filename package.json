{
    "name": "yy-input",
    "version": "0.11.1",
    "description": "basic input support for touch, mouse, and keyboard",
    "main": "index.js",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "test": "budo --dir docs docs/code.js:index.js --live",
